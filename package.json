	"scripts": {
		"init": "clear",
		"prestart": "npm run init",
		"start": "cross-env NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm --no-warnings --experimental-specifier-resolution=node\" webpack serve -c webpack.config.ts",
		"prebuild": "del-cli build",
		"build": " cross-env NODE_OPTIONS=\"-r dotenv/config --loader ts-node/esm --no-warnings --experimental-specifier-resolution=node\" webpack -c webpack.config.ts",
		"host": "cross-env NODE_OPTIONS=\"-r dotenv/config\" serve build",
		"build-host": "npm run build && npm run host",
		"build-then-upload": "npm-run-all init build script:upload-to-s3",
		"script:upload-to-s3": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm -r dotenv/config scripts/upload-to-s3.ts",
		"sync": "git checkout . && git remote update && git pull",
		"push": "git add -A && git commit -m \"Update\" && git push"
	},
